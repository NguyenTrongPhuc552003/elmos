{
  "version": 2,
  "public": false,
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "docs/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ]
}